{
  "name": "new-v1-addon",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache\"",
    "lint:css:fix": "stylelint \"**/*.css\" --allow-empty-input --fix",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo \"A v2 addon does not have tests, run tests in test-app\""
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.9.0",
    "decorator-transforms": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/runtime": "^7.26.10",
    "@embroider/addon-dev": "^7.1.3",
    "@eslint/js": "^9.23.0",
    "@ijlee2-frontend-configs/ember-template-lint": "^0.5.2",
    "@ijlee2-frontend-configs/eslint-config-ember": "^0.4.0",
    "@ijlee2-frontend-configs/prettier": "^0.2.3",
    "@ijlee2-frontend-configs/stylelint": "^0.2.3",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.4.0",
    "concurrently": "^9.1.2",
    "ember-template-lint": "^7.0.1",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "rollup": "^4.37.0",
    "rollup-plugin-copy": "^3.5.0",
    "stylelint": "^16.16.0"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {},
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ]
}
