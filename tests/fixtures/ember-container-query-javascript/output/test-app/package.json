{
  "name": "test-app",
  "version": "3.2.0",
  "description": "Container queries using Ember modifiers",
  "keywords": [
    "container-queries",
    "container-query",
    "element-queries",
    "element-query",
    "ember-octane",
    "emberjs",
    "glint",
    "responsive-design"
  ],
  "repository": {
    "type": "git",
